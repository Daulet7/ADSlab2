#include <iostream>
#include <queue>

using namespace std;

int main(){
    int n, k;
    cin >> n >> k;

    queue<string> poem;

    for(int i = 0; i<n; i++){
        string a;
        cin >> a;
        poem.push(a);
    }

    for(int i =0; i<k; i++){
        string b = poem.front();
        poem.pop();
        poem.push(b);
    }

    for(int i = 1; i<=n; i++){
        cout << poem.front() << " ";
        poem.pop();
    }

    return 0;
}

