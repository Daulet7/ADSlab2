#include <iostream>
#include <vector>

using namespace std;

int search(int n){
    vector<int> seq(n);
    for(int i = 0; i<n; i++){
        cin >> seq[i];
    }

    int k;
    cin >> k;

    int d = k - seq[n-1];
    int min = abs(d);
    int value = seq[n-1];

    for(int i = n-2; i>=0; i--){
        int t = k - seq[i];
        int maybe = abs(t);
        if(maybe <= min){
            min = maybe;
            value = seq[i];
        }
    }

    int answer;

    for(int i = 0 ;i <n; i++){
        if(value == seq[i]){
            answer = i;
            break;
        }
    }

    return answer;
}

int main(){
    int n;
    cin >> n;
    int answer = search(n);
    cout << answer;
    return 0;
}


