#include <iostream>
#include <vector>

using namespace std;

int main(){
    int n;
    cin >> n;

    vector<pair<int, int>> seq;
    for(int i = 0; i < n; i++){
        int a;
        cin >> a;

        seq.push_back({0, a});
    }

    for(int i = 0; i<n; i++){
        if(i%2 !=0){
            seq[i].first = 1;
        }
    }

    for(int i = 0; i<n; i++){
        if(seq[i].first == 0){
            cout << seq[i].second << " ";
        }
    }

    return 0;

}
DDDDDDDDD
#include <iostream>
#include <map>
#include <algorithm>

using namespace std;

void mode(int n){
    map<int,int> seq;

    for(int i = 0; i<n; i++){
        int a; 
        cin >> a;
        seq[a]++;
    }

    int md = 0;
    for(auto i = seq.begin(); i != seq.end(); ++i){
        md = max(md, i -> second);
    }
    
    for(auto i = seq.rbegin(); i != seq.rend(); ++i){
        if(i -> second == md){
            cout << i -> first << " ";
        }
    }
}

int main(){
    int n; 
    cin >> n;
    mode(n);
    return 0;
}
